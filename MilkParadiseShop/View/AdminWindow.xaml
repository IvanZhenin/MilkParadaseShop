<Window x:Class="MilkParadiseShop.View.AdminWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MilkParadiseShop.View"
        mc:Ignorable="d"
        MinHeight="650" MinWidth="1100">
    <Grid>
        <Grid.Background>
            <LinearGradientBrush>
                <GradientStop Color="HotPink" Offset="0.4"/>
                <GradientStop Color="Coral" Offset="0.8"/>
            </LinearGradientBrush>
        </Grid.Background>
        <Border CornerRadius="20" Margin="10" Background="AntiqueWhite"/>
        <Frame Name="AdminDataPanelFrame" Style="{StaticResource FrameOrder}"/>
    </Grid>
</Window>
